{
  "extends": "default",
  "rules": {
    "avoid-throw": false,
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "indent": ["warn", 4],
    "compiler-fixed": false,
    "not-rely-on-time": false,
    "quotes": ["error", "single"],
    "no-empty-blocks": false,
    "no-complex-fallback": false
  }
}
